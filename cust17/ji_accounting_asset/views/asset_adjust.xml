<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="view_asset_adjust_tree" model="ir.ui.view">
        <field name="name">asset.adjust.tree</field>
        <field name="model">asset.adjust</field>
        <field name="arch" type="xml">
            <tree string="資產調整作業">
                <field name="name"/>
            </tree>
        </field>
    </record>

    <record id="view_asset_adjust_form" model="ir.ui.view">
        <field name="name">asset.adjust.form</field>
        <field name="model">asset.adjust</field>
        <field name="arch" type="xml">
            <form string="資產調整作業">
                <sheet>
                    <group>
                        <field name="name"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>
</odoo>