<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record id="seq_asset_master" model="ir.sequence">
            <field name="name">資產主檔單號</field>
            <field name="code">asset.master</field>
            <field name="prefix">FA%(y)s%(month)s</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>
        <record id="seq_asset_depreciation" model="ir.sequence">
            <field name="name">資產折舊單號</field>
            <field name="code">asset.depreciation</field>
            <field name="prefix">FD-%(y)s%(month)s</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>
        <record id="seq_asset_disposal" model="ir.sequence">
            <field name="name">資產作廢單號</field>
            <field name="code">asset.disposal</field>
            <field name="prefix">F1-%(y)s%(month)s</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>
        <record id="seq_asset_adjust" model="ir.sequence">
            <field name="name">資產調整單號</field>
            <field name="code">asset.adjust</field>
            <field name="prefix">F2-%(y)s%(month)s</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>
        <record id="seq_asset_sale" model="ir.sequence">
            <field name="name">資產出售單號</field>
            <field name="code">asset.sale</field>
            <field name="prefix">F3-%(y)s%(month)s</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>